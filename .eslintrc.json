{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console" : "off",
        "linebreak-style": 0,
        "no-underscore-dangle" : 0 ,
        "no-restricted-globals" : 0 ,
        "react/jsx-filename-extension": [0],
        "import/extensions": "off"     
    },
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
          }
        }
      }
}
